# set number of nodes
set opt(nn) 2.0

# set activity file
set opt(af) $opt(config-path)
append opt(af) /activityfile.tcl

# set mobility file
set opt(mf) $opt(config-path)
append opt(mf) /mobilityfile.tcl

# set start/stop time
set opt(start) 0.0
set opt(stop) 40.0

# set floor size
set opt(x) 2500
set opt(y) 519
set opt(min-x) 0
set opt(min-y) 15
